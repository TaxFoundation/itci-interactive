    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-tagmanager": "^3.13.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-netlify": "^3.14.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-styled-components": "^4.13.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-transformer-csv": "^3.13.0",
    "gatsby-transformer-sharp": "^3.13.0",
    "gatsby-transformer-yaml": "^3.13.0",
    "lodash": "^4.17.21",
    "npm": "^7.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.1",
    "styled-reset": "^4.3.4",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-wesbos": "2.0.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TaxFoundation/itci-interactive"
  },
  "bugs": {
    "url": "https://github.com/TaxFoundation/itci-interactive/issues"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "jsx-a11y/label-has-for": "off",
      "jsx-a11y/label-has-associated-control": [
        2,
        {
          "assert": "either",
          "depth": 3
        }
      ]
    }
  }
}
